<!--
Square Connect API

Client library for accessing the Square Connect APIs

OpenAPI spec version: 2.0
Contact: developers@squareup.com
-->
<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net461</TargetFramework>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\$(AssemblyName).xml</DocumentationFile>
    <NuspecFile>Square.Connect.nuspec</NuspecFile>
    <Authors>SkuVault</Authors>
    <PackageId>Square.Connect.SV</PackageId>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/skuvault/Square.Connect.SV</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/skuvault/Square.Connect.SV/blob/master/LICENSE</PackageLicenseUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CSharp" Version="4.5.0" />
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
    <PackageReference Include="RestSharp" Version="106.3.1" />
    <PackageReference Include="System.ComponentModel.Annotations" Version="4.5.0" />
  </ItemGroup>
</Project>
